function pattern(num){
    let str = '';
    for (let j= 1; j<=num; j++){
        str+=j
        console.log(str);
    }    
}

function pattern1(num){
    for (i=1;i<=num;i++){
        let str='';
        for (j=1;j<=i;j++){
            str+=j;
            
        }   
        console.log(str);
    }
}
